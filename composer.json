        "mobile",
        "phone",
        "form",
        "validation",
        "geocoding",
        "geolocation"
    ],
    "homepage": "https://github.com/defro/codeigniter-phonenumber",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "JoÃ«l Gaujard",
